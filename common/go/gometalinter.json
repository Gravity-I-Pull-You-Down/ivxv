{
	"test": true,
	"cyclo": 25,
	"linelength": 120,
	"deadline": "5m",
	"enablegc": true,
	"linters": {
		"gosec": { "command": "gosec -fmt=csv -exclude=G104" }
	},
	"exclude": [
		"error return value not checked \\(fmt\\.Fprint(f|ln)?\\(os\\.Stderr,"
	],
	"disable": [
		"gochecknoglobals",
		"gochecknoinits",
		"misspell",
		"nakedret",
		"safesql",
		"test",
		"testify"
	]
}
